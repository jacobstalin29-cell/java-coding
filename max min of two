6 question 
import java.util.Scanner;

public class MaxMinOfTwo {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        // Input
        int a = sc.nextInt();
        int b = sc.nextInt();

        // Find max and min
        int max = (a > b) ? a : b;
        int min = (a < b) ? a : b;

        // Output
        System.out.println("Max=" + max + " Min=" + min);

        sc.close();
    }
}
