7 question 
import java.util.Scanner;

public class MaxOfThree {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        // Input
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();

        // Find maximum
        int max = a;
        if (b > max) {
            max = b;
        }
        if (c > max) {
            max = c;
        }

        // Output
        System.out.println(max);

        sc.close();
    }
}
