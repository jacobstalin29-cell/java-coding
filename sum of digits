21 question 
import java.util.Scanner;

public class SumOfDigits {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        // Input
        int n = sc.nextInt();

        // Handle negative numbers
        n = Math.abs(n);

        int sum = 0;

        // Sum digits
        while (n > 0) {
            sum += n % 10;
            n /= 10;
        }

        // Output
        System.out.println(sum);

        sc.close();
    }
}
